# Verificar HDFS
docker compose exec hadoop-namenode hdfs dfs -ls /data/

# Verificar YARN
docker compose exec resourcemanager yarn application -list

# Verificar servicios
docker compose ps

# Ejecutar análisis MapReduce
docker compose exec mapreduce-processor python mapreduce_processor.py


